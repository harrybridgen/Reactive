{
  "name": "rx-vscode",
  "displayName": "Reactive",
  "description": "Syntax highlighting for Reactive language",
  "version": "0.0.1",
"engines": {
  "vscode": "^1.107.0"
},
  "categories": [
    "Programming Languages"
  ],

  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "reactive",
        "aliases": [
          "Reactive"
        ],
        "extensions": [
          ".rx"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "reactive",
        "scopeName": "source.reactive",
        "path": "./syntaxes/reactive.tmLanguage.json"
      }
    ]
  },

  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "typescript": "^5.9.3"
  }
}
